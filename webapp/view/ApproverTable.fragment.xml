<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Table noDataText="{i18n>masterListNoDataText}" id="tableApprove" items="{Data>ApproverSet}">
		<headerToolbar>
			<OverflowToolbar>
				<Title text="{i18n>detailIconTabBarAppr} {Data>WfWiid}" level="H2"/>
			</OverflowToolbar>
		</headerToolbar>		
		<items>
			<ColumnListItem type="Active" id="tableApproveItem">
				<cells>
					<Text text="{Data>RoundNumber}"/>
					<Text text="{Data>OrderNumber}"/>
					<Text text="{Data>IdPosText}"/>
					<Text text="{Data>Pernr}"/>
					<Text text="{Data>Uname}"/>
					<Text text="{Data>Person}"/>
					<ObjectStatus
								text="{Data>StatusText}"
								state="{
									path: 'Data>Status',
									formatter: '.formatter.approveStatus'
								}" />
					<Text text="{ path: 'Data>DateReceived', type: 'sap.ui.model.type.Date', formatOptions: { style: 'short', pattern: 'dd.MM.yyyy' } }"/>
					<Text text="{ path: 'Data>TimeRecevied', type: 'sap.ui.model.odata.type.Time', formatOptions: { pattern: 'HH:mm:ss' } }"/>
					<Text text="{ path: 'Data>DateFinish', type: 'sap.ui.model.type.Date', formatOptions: { style: 'short', pattern: 'dd.MM.yyyy' } }"/>
					<Text text="{ path: 'Data>TimeFinish', type: 'sap.ui.model.odata.type.Time', formatOptions: { pattern: 'HH:mm:ss' } }"/>
				</cells>
			</ColumnListItem>
		</items>
		<columns>
			<Column>
				<header>
					<Label text="{i18n>Kolo}"/>
				</header>
			</Column>
			<Column>
				<header>
					<Label text="{i18n>Poradi}"/>
				</header>
			</Column>
			<Column>
				<header>
					<Label text="{i18n>Pozice}"/>
				</header>
			</Column>			
			<Column>
				<header>
					<Label text="{i18n>Pernr}"/>
				</header>
			</Column>
			<Column>
				<header>
					<Label text="{i18n>UserId}"/>
				</header>
			</Column>
			<Column>
				<header>
					<Label text="{i18n>Osoba}"/>
				</header>
			</Column>
			<Column>
				<header>
					<Label text="{i18n>StatusTxt}"/>
				</header>
			</Column>
			<Column>
				<header>
					<Label text="{i18n>DatsReceived}"/>
				</header>
			</Column>
			<Column>
				<header>
					<Label text="{i18n>TimsReceived}"/>
				</header>
			</Column>
			<Column>
				<header>
					<Label text="{i18n>DatsFinish}"/>
				</header>
			</Column>
			<Column>
				<header>
					<Label text="{i18n>TimsFinish}"/>
				</header>
			</Column>
		</columns>
	</Table>
</core:FragmentDefinition>