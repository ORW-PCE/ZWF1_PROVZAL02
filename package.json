{
    "name": "zwf1.prozal02",
    "version": "0.0.1",
    "description": "",
    "devDependencies": {
        "@sap/ui5-builder-webide-extension": "1.0.x",
        "@sap/ux-ui5-fe-mockserver-middleware": "1",
        "@sap/ux-ui5-tooling": "1",
        "@ui5/cli": "^2.14.1",
        "rimraf": "3.0.2",
        "ui5-task-flatten-library": "0.2.5"
    },
    "scripts": {
        "build_old": "ui5 build --clean-dest --include-task=generateManifestBundle generateCachebusterInfo",
        "start": "fiori run --open \"test/flpSandbox.html?sap-client=001&sap-ui-xx-viewCache=false&sap-language=CS&saml2=disabled#zwf1_pz01_app-create\"",
        "start-local": "fiori run --config ./ui5-local.yaml --open \"test/flpSandbox.html?sap-client=800&sap-ui-xx-viewCache=false#zwf1_pz01_app-create\"",
        "start-noflp": "fiori run --open \"index.html?sap-client=001&sap-ui-xx-viewCache=false\"",
        "start-variants-management": "fiori run --open \"preview.html?sap-client=001&sap-ui-xx-viewCache=false&fiori-tools-rta-mode=true&sap-ui-rta-skip-flex-validation=true#preview-app\"",
        "build": "ui5 build -a --clean-dest --include-task=generateManifestBundle generateCachebusterInfo",
        "deploy": "npm run build && fiori deploy --config ui5-deploy.yaml && rimraf archive.zip",
        "deploy-config": "fiori add deploy-config",
        "start-mock": "fiori run --config ./ui5-mock.yaml --open \"test/flpSandboxMockServer.html?sap-ui-xx-viewCache=false#zwf1_pz01_app-create\"",
        "suite-tests": "fiori run --open 'test/testsuite.qunit.html'",
        "unit-tests": "fiori run --open 'test/unit/unitTests.qunit.html'",
        "int-tests": "fiori run --open 'test/integration/opaTests.qunit.html'",
        "undeploy": "fiori undeploy --config ui5-deploy.yaml",
        "deploy-test": "npm run build && fiori deploy --config ui5-deploy.yaml --testMode true"
    },
    "ui5": {
        "dependencies": [
            "@sap/ux-ui5-tooling",
            "@sap/ux-ui5-fe-mockserver-middleware",
            "ui5-task-flatten-library"
        ]
    },
    "private": true,
    "sapuxLayer": "CUSTOMER_BASE",
    "keywords": [
        "ui5",
        "openui5",
        "sapui5"
    ],
    "main": "webapp/index.html",
    "dependencies": {
        "yo": "^4.3.1"
    }
}
